ghci <<EOF
:set +t
5 + 8 -- 13 :: Integer
3 * 5 + 8 -- 23 :: Integer
2 + 4 -- 6 :: Integer
(+) 2 4 -- 6 :: Integer
sqrt 16 -- 4.0 :: Double
succ 6 -- 7 :: Integer
succ 7 -- 8 :: Integer
pred 9 -- 8 :: Integer
pred 8 -- 7 :: Integer
sin (pi / 2) -- 1.0 :: Double
round 3.5 -- 4 :: Integer
round 3.4 -- 3 :: Integer
floor 3.7 -- 3 :: Integer
ceiling 3.3 -- 4 :: Integer
EOF
